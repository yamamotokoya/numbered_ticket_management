<tr>
  <th scope="row"><%= user.id %></th>
  <td><%= link_to user.name, admin_user_path(user), style: "color: black;" %></td>
  <td><%= user.email %></td>
  <td></td>
  <% if user.checked_in_yet?(@viewing_time) %>
    <td><%= render partial: 'checkin_form', locals: {user: user, viewing_time: @viewing_time} %></td>
  <% else %>
    <td><button type="button" class="btn btn-secondary">受付済</button></td>
  <% end %>
</tr>
